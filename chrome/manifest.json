{
    "name": "Litcode",
    "version": "1.0",
    "description": "Race against friends to solve Leetcode questions.",
    "manifest_version": 2,
    "background": {
        "persistent": true,
        "scripts": [
            "socket.io.js",
            "background.js"
        ]
    },
    "permissions": [
        "tabs"
    ],
    "page_action": {
        "default_title": "Litcode",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://leetcode.com/*",
                "https://leetcode.com/*"
            ],
            "js": [
                "jquery.js",
                "contentscript.js"
            ]
        }
    ]
  }
